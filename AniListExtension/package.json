{
  "name": "wxt-svelte-starter",
  "description": "manifest.json description",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:chrome": "wxt -b chrome",
    "build": "pnpm run generate && wxt build",
    "build:chrome": "pnpm run generate && wxt build -b chrome",
    "zip": "wxt zip",
    "zip:chrome": "wxt zip -b chrome",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postinstall": "wxt prepare",
    "generate": "graphql-codegen"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^5.0.4",
    "@wxt-dev/module-svelte": "^2.0.3",
    "svelte": "^5.38.1",
    "svelte-check": "^4.3.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "wxt": "^0.20.8"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@wxt-dev/auto-icons": "^1.1.0",
    "bootstrap": "^5.3.7"
  }
}
